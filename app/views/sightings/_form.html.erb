

<%= form_for [@species, @sighting] do |f| %>
  <%= f.label :longitude %>
  <%= f.text_field :longitude %>
  <%= f.label :latitude %>
  <%= f.text_field :latitude %>
  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.select :region, {"Mt Hood" => "Mt Hood",
     "Sandy" => "Sandy",
     "Columbia Gorge" => "Columbia Gorge"} %>

  <p><%= f.submit %></p>
<% end %>
